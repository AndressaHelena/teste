package treinamento.com;

public class NotaAluno {

	public static void main(String[] args) {
		// graduação A=8 B=7 C=6 D=0ate5
		int notaAluno = 6,5;
		
		if(notaAluno >= 8) {
			System.out.println("Graduação A");
		}else if(notaAluno < 8 && notaAluno >= 7) {
			System.out.println("Graduação B");
		}else if(notaAluno < 7 && notaAluno >= 6) {
			System.out.println("Graduação C");
		}else {
			System.out.println("Graduação D");
		}
	}
}
		
