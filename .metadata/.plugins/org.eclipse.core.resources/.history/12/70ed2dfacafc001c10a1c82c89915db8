package treinamento.com;

public class teste {

	public static void main(String[] args) {
		int num = 5, count = 1;
        while(count <= 3) {
        	 System.out.println(num);
        	 System.out.println("cont " + count);
            ++count;
            num += count;
        }
        System.out.println(num);
		
	}

}
